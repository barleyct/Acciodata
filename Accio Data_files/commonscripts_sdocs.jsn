/******************************************************************************
 * This file includes the code that makes the accio checkbox widget found
 * on the checked individuals list page.
 * The accio-checkbox is a checkbox that has multiple states.
 * 
 * The checkbox has 4 states ('', 'X', 'L', 'B'):  blank, checked, locked, banned
 * 
 * <accio-checkbox        // tag
 *   name=complete        // name of the input (for the form)
 *   value='C'            // initial state
 *   id=complete          // DOM id (which should always be given)
 *   exclude=X            // a comma delimited list of states to disallow for this checkbox
 *  />
 *    
 *  Sometimes we have a checkbox where it does not make sense to allow one of the states.
 *  For example, if we have a search criteria that automatically excludes a bunch of searches,
 *  but doesn't have the capacity to add any searches (such as New Updates Only), then
 *  set exclude=X.  To exclude the unchecked value set exclude=blank.
 *
 *  This file automatically finds all accio-checkbox tags and processes them on page load.
 *  However, if an accio-checkbox is added after the page loads, run enable_accio_checkbox
 *  on the element
 *  
 *
 *  To programmatically set the value of a checkbox, use the set_accio_checked method.
 */

/******************************************************************************
 * handles when a reset is called on the form containing this checkbox
 *
 * arguments:
 *  e - the event object
 *
 * returns nothing
 */
function accio_checkbox_reset_handler(e) {
  // Get all accio-checkbox objects in this form and reset them to their original values
  var $boxes = jQuery(e.delegateTarget).find('accio-checkbox') 
  $boxes.each(function(i,el) {
    set_accio_checked(el.getAttribute('id'),el.getAttribute('original-value')) 
  });
}


/******************************************************************************
 * method to turn on an accio-checkbox tagged element
 * This method is called on document load to render all of the accio-checkbox tags on the page.
 * It can also be called after page load to render a new accio-checkbox element.
 *
 * Arguments:
 *   el - the DOM element (with an accio-checkbox tag) to turn into an accio-checkbox.
 *
 * Returns:
 *   nothing
 */
function enable_accio_checkbox(el) {
  // enable the tag
  document.createElement('accio-checkbox'); // IE requires this step, other browsers don't care
  var checkbox_tag_set = document.getElementsByTagName('accio-checkbox');

  /* if this element has already been managed by this code, skip it */
  if(el.attributes['accio-managed']) { return; }
    
  // if a tag does not have a name,
   // skip the tag.  It is mal-formed.
   if ((!el.getAttribute('name')) || (el.getAttribute('name').length < 1)) {
     return;
   }
   var name = el.getAttribute('name');

   var excludes = el.getAttribute('accio-exclude');
   if(excludes) {
     excludes = excludes.split(',');
   }
   var checked='';
   if(el.hasAttribute('checked')) {
     checked = el.getAttribute('checked');
     if(! checked ) {
       checked = 'X'; // default if checked
     }
   }
   
   checked = accio_checkbox_check_exclude(checked,excludes);

   var id = el.getAttribute('id');
   if( !id ) {
     id = '';
   }
   var inner_id = id;
   var inner_span_id = id + '_span_id';
   if( id.length > 0 ) {
     inner_id = id + '_inner_id';      
     inner_span_id = id + '_span_id';
   } else {
     inner_id = name + '_inner_id';      
     inner_span_id = name + '_span_id';
   }

   el.setAttribute('accio-managed','Y'); // Indicate that this is a managed node now

   el.setAttribute('original-value',checked);

   // if this checkbox has a related form, attach an on reset handler to that
   // form to reset this checkbox to its original state
   var $el = jQuery(el)

   var $form = $el.closest('form')
   if($form.length > 0) {
     if(!($form.attr('accio-checkbox-managed'))) {
       $form.attr('accio-checkbox-managed','X')
       $form.bind('reset',accio_checkbox_reset_handler);
     }
   }
   // at each tag instance, install a click handler, and display an icon or some Markup
   el.innerHTML = 
     "<span class='accio-checkbox accio-checkbox-unchecked noselect' id='" + inner_span_id + "'>" + accio_checkbox_render_checkbox(checked)+"</span>" + 
     "<input type=hidden class='accio-regular-checkbox' name='" + name + "' id='" + inner_id + "' value='" + checked + "' >";

   $span = $el.find('span');
   $span.data('excludes',excludes);
   $span.click(function (e) {
     accio_checkbox_click_handler(e);
   });

}

/******************************************************************************
 * method to render the icon (HTML) for a given checkbox state
 *
 * arguments:
 *   One of '','X','L', or 'B'
 * 
 * returns:
 *   HTML markup
 */
function accio_checkbox_render_checkbox(state) {
  switch(state) {
    case 'L':
      return "<i class='fa fa-lock'></i>"; 
    case 'X':
      return "<i class='fa fa-check-square-o'></i>"; 
    case 'B':
      return "<i class='fa fa-ban'></i>"; 
  }
  return "<i class='fa fa-square-o'></i>"; 
}

/******************************************************************************
 * method to change the state of an accio checkbox
 * This method may be called by external processes to change checkboxes.
 *
 * arguments:
 *   id = the DOM id of the accio-checkbox
 *   value = one of '', 'X', 'L', or 'B'
 *  
 * returns:
 *   nothing
 */ 
function set_accio_checked(id,value) {
  var $el = jQuery('#' + id);
  if($el.length < 1) { return; }
  enable_accio_checkbox($el[0]);
  accio_checkbox_actually_change_checkbox(id,value)
  $el.attr('original-value',value);
}

/******************************************************************************
 * method to determine if a value of a checkbox is excluded (not allowed), 
 * and to return a value that is allowed. 
 * If the blank value ('') should be excluded, include 'blank' in the array
 * of excluded values.
 *
 * arguments:
 *   val = the proposed value of the checkbox (one of '','X','L','B')
 *   excludes = an array of excludes i.e. ['X'], or ['L','B'], or ['blank'] ...
 *  
 * returns:
 *   one of '', 'X', 'L', 'B' reflecting the correct state of the checkbox
 */
function accio_checkbox_check_exclude(val,excludes) {
  if(!excludes) { return val; }
  if(typeof excludes.indexOf !== 'function') { return val; }
  var loops = 0;
  while(loops < 4) {
    switch(val) {
      case 'X' : 
         if(excludes.indexOf('X') !== -1) { val = 'L'; }
         break;
      case 'L' : 
         if(excludes.indexOf('L') !== -1) {val = 'B'; }
         break;
      case 'B' : 
         if(excludes.indexOf('B') !== -1) {val = ''; }
         break;
      default :                               
         if(excludes.indexOf('blank') !== -1) {val = 'X';}
    
    }
    loops++;
  } 
  return val;
}

/******************************************************************************
 * accio_checkbox_click_handler - the handler for when someone clicks 
 * an accio-checkbox.
 *
 * arguments:
 *   e = event object for the click
 *
 * returns:
 *   nothing
 *
 */
function accio_checkbox_click_handler(e) {
  var $span = jQuery(e.currentTarget); // event is on the span
  var $input = $span.parent().find('input');
  var id = $span.parent().attr('id');
  var newval;
  var newclass = 'accio-checkbox-checked'

  var excludes = $span.data('excludes');

  switch($input.val()) {
    case 'X': newval = 'L';
      break;
    case 'L': newval = 'B';
      break;
    case 'B': newval = '';
      break;
    default: newval = 'X';
      break;
  }
  
  newval = accio_checkbox_check_exclude(newval,excludes);
 
  if(accio_checkbox_should_i_stop_the_user(id,newval,window.selection_schema)) {
    var autocorrect = document.getElementById('autocorrect');
    if(autocorrect && autocorrect.checked) {
       accio_checkbox_actually_change_checkbox(id,newval);
       accio_checkbox_fix_conflicts(id,newval);
    } else {
      accio_checkbox_alert_user(id,newval,window.selection_schema);
    }
  } else {
    accio_checkbox_actually_change_checkbox(id,newval);
  }

  e.preventDefault();
}

/******************************************************************************
 * callback for when someone clicks the cancel button on the 
 * accio-checkbox dialog
 *
 * arguments:
 *   id = DOM id of the accio-checkbox element
 *   e = event object from click
 *
 * returns:
 *   nothing
 */
function accio_checkbox_cancel_confirm(id, e) {
  accio_checkbox_close_confirm_dialog(id);
  e.preventDefault();
}

/******************************************************************************
 * close the accio_checkbox dialog (does not affect checkboxes)
 *
 * arguments:
 *   id = DOM id of the accio-checkbox DOM element that causes this dialog
 *        to appear.
 *  
 * returns:
 *   nothing
 */
function accio_checkbox_close_confirm_dialog(id) {
  hide_modal_overlay('accio_checkbox_confirm_dialog_backdrop');
  var $dialog = jQuery('#accio_checkbox_confirm_dialog_'+id);
  $dialog.remove();
}

/******************************************************************************
 * fix conflicting checkbox settings based on the new state of this checkbox.
 *
 * arguments:
 *   id = DOM id of the accio-checkbox element
 *   new_state = is one of 'L', '', 'B', or 'X' - the new state of the
 *               checkbox identified by id
 *
 */
function accio_checkbox_fix_conflicts(id,new_state) {
  // Only do this if there is a checkbox 'schema' to work with.
  if(window['selection_schema'] && window.selection_schema[id] && window.selection_schema[id]['excludes']) {
    switch(new_state) {
      case 'L':
        window.selection_schema[id]['excludes'].forEach(function(v,i) {
          accio_checkbox_actually_change_checkbox(v,'');
        });
        break;
      case 'X':
        window.selection_schema[id]['excludes'].forEach(function(v,i) {
          if( jQuery('#'+v+'_inner_id').val() == 'L' ) {
            accio_checkbox_actually_change_checkbox(v,'X'); // change a lock (L) to check (X)
          }
        });
        break;
      default:
        break;
    }
  } 
}

/******************************************************************************
 * callback for when someone clicks the change all button on the 
 * accio-checkbox dialog
 *
 * arguments:
 *   id = DOM id of the accio-checkbox element
 *   e = event object from click
 *
 * returns:
 *   nothing
 */
function accio_checkbox_confirm_change_all(id,new_state,e) {
  accio_checkbox_close_confirm_dialog(id);
  accio_checkbox_actually_change_checkbox(id,new_state);

  // we are going to change all of the other conflicting checkboxes as well
  accio_checkbox_fix_conflicts(id,new_state);

  e.preventDefault();
}

/******************************************************************************
 * callback for when someone clicks the change one button or the uncheck button
 * on the accio-checkbox dialog.
 *
 * arguments:
 *   id = DOM id of the accio-checkbox element
 *   e = event object from click
 *
 * returns:
 *   nothing
 */
function accio_checkbox_confirm_change_one(id,new_state,e) {
  accio_checkbox_close_confirm_dialog(id);
  accio_checkbox_actually_change_checkbox(id,new_state);
  e.preventDefault();
}

/******************************************************************************
 * render the confirmation dialog to the user when they select an
 * inconsistent state for one of the accio-checkbox elements.
 *
 * arguments:
 *   id = DOM id of the accio-checkbox element
 *   newval = One of '','X','L' or 'B'
 *   constraints = a JSON structure containing the constraints for the
 *                 checkboxes.
 *
 * returns:
 *   nothing
 */
function accio_checkbox_alert_user(id,newval,constraints) {

    var template = '<div class="accio-checkbox-dialog" id="accio_checkbox_confirm_dialog___id__"><div class="panel panel-primary">'
    template += '<div class="panel-heading"><div><div class="pull-right" onclick="accio_checkbox_cancel_confirm(\'__id__\',event)"><i class="fa fa-close fa-lg"></i></div><h4>'+edittext('1wo')+'</h4></div></div>';
    template += '<div class="panel-body"><div class"container"><div class="row">';
    template += '<div class="accio-checkbox-dialog-content">You are about to check or lock a search criteria (<span class=\'capitalize\'>__new_value__</span>) '; 
    template += 'that is incompatible with other search criteria that are already checked or locked. ';
    template += 'Please indicate if you wish to continue, by either:<br><ol style=\'list-style-type: lower-alpha\'>';
    template += '<li>Set this criteria (<span class=\'capitalize\'>__new_value__</span>) to ignore (<i class=\'fa fa-square-o\'></i>).</li>';
    template += '<li>Simply locking or checking the one criteria (<span class=\'capitalize\'>__new_value__</span>).</li>';
    template += '<li>Checking or locking the search criteria (<span class=\'capitalize\'>__new_value__</span>) and un-checking or unlocking the other criteria.</li>';
    template += '<li>Cancel this operation.</li>';
    template += '</ol></div>';
    template += '</div><div class="row"><div class="col-md-12">';
    template += '<hr>';
    template += '<div class="pull-right">';
    template += '<div class="btn-group"><button class="btn btn-info" onclick="accio_checkbox_confirm_change_one(\'__id__\',\'\',event)" id="__id___save">Uncheck this criteria (<i class=\'fa fa-square-o\'></i>)</button></div>&nbsp;&nbsp;';
    template += '<div class="btn-group"><button class="btn btn-success" onclick="accio_checkbox_confirm_change_one(\'__id__\',\'__new_state__\',event)" id="__id___save">Only __new_state_action__ this criteria</button></div>&nbsp;&nbsp;';
    template += '<div class="btn-group"><button class="btn btn-success" onclick="accio_checkbox_confirm_change_all(\'__id__\',\'__new_state__\',event)" id="__id___save">Change Other Criteria</button></div>&nbsp;&nbsp;';
    template += '<div class="btn-group"><button class="btn btn-danger" onclick="accio_checkbox_cancel_confirm(\'__id__\',event)">Cancel</button></div>'; 
    template += '</div>';
    template += '</div></div></div></div>';
    template += '</div></div>';

    // create the dialog in the owning object (usually the BODY)
    var $owner = jQuery('body');
  
    // create a grey overlay so that the dialog is modal
    show_modal_overlay('accio_checkbox_confirm_dialog_backdrop',99); // the zindex for the dialog is set in style (to 100)
  
    var action_name = 'lock (<i class="fa fa-lock"></i>)';
    if(newval == 'X') {
      action_name = 'check (<i class="fa fa-check-square-o"></i>)';
    }
    
    // instantiate the modal dialog template
    template = template.replace(/__id__/g,id);
    template = template.replace(/__new_state__/g,newval);
    template = template.replace(/__new_value__/g,id.replace('/_/g',' '));
    template = template.replace(/__new_state_action__/g,action_name);

    // attach the dialog to the DOM
    $owner.append(template);
}

/******************************************************************************
 * method that checks for inconsistencies in the checkbox states
 * given a new state for the checkboxes.
 *
 * arguments:
 *   id = DOM id of the checkbox that is being changed
 *   newval = one of '', 'X', 'L', 'B'
 *   constraints = a JSON structure containing the constraints for the
 *
 * returns:
 *   true or false
 */
function accio_checkbox_should_i_stop_the_user(id,newval,constraints) {
  // Right now, there are no situations where a transition to anything
  // other than a lock will cause a conflict
  if((newval != 'L') && (newval != 'X')) {
    return false;
  }

  if(!constraints || !constraints[id] || !constraints[id]['excludes']) {
    return false;
  }

  var retval = false;
  if(newval == 'L') {
    constraints[id]['excludes'].forEach(function(v,i) {
      var $input = jQuery('#'+v+'_inner_id');
      if( $input && ($input.length > 0) && ($input.val().length > 0)) {
        retval = true;
      }  
    });
  } else { // newval == 'X'
    constraints[id]['excludes'].forEach(function(v,i) {
      var $input = jQuery('#'+v+'_inner_id');
        // has some other setting exclusively locked out this setting?
      if( $input && ($input.length > 0) && ($input.val() == 'L')) { 
        retval = true;
      }  
    });
  }

  return retval;
}

/******************************************************************************
 * method to change the value of a checkbox after the user has clicked it
 * or has made a selection in the confirmation dialog.  This is usually
 * called from a callback.
 *
 * arguments:
 *   id = DOM id of the accio-checkbox
 *        This DOM element should have been managed with a call to
 *        enable_accio_checkbox - usually at page load.
 *   newval = The new value for the checkbox ('','X','L','B')
 *
 * returns:
 *   nothing
 */
function accio_checkbox_actually_change_checkbox(id,newval) {
  var $span = jQuery('#'+id+'_span_id'); // event is on the span
  var $input = $span.parent().find('input');

  $span.removeClass('accio-checkbox-checked');
  $span.removeClass('accio-checkbox-unchecked');
  $span.removeClass('accio-checkbox-locked');
  $span.removeClass('accio-checkbox-banned');

  var newclass = '';
  switch(newval) {
    case 'X': 
      newclass = 'accio-checkbox-checked';
      break;
    case 'L': 
      newclass = 'accio-checkbox-locked';
      break;
    case 'B': 
      newclass = 'accio-checkbox-banned';
      break;
    default:
      newclass = 'accio-checkbox-unchecked';
      break;
  }
  
  $input.val(newval);
  $span.addClass(newclass);
  $span.html(accio_checkbox_render_checkbox(newval)); 
}

