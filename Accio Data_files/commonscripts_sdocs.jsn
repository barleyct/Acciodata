    var html = "";

    // Load the pricebook
    for(var idx = 0;idx < package_list.length;idx++) {
      pname = package_list[idx];

      html += '<option value="' + pname + '">'+pname+'</option>';
    }
    jQuery('#orc_pkgsetselid').find('option').remove().end().append(html);

    // Setup the account autocomplete function
    jQuery("#orc_accountvar").autocomplete({
      source:'/c/s2/complete_accountbyaccount',
      select: function(event,ui) {
        var selectedObj = ui.item;
        var val = selectedObj.value;
        var res = val.split('|');
        jQuery("#orc_accountvar").val(res[0]);
        orc_on_account_changed(res[0]);
        return false;
      }
    });

    // Setup the account changed event handler
    jQuery("#orc_accountvar").on("change",function(event) {
      orc_on_account_changed(jQuery("#accountvar").val());
    });

    jQuery("#orc_accountvar").on("keydown",function(event){
      if (event.keyCode == 13) {
        event.preventDefault();
        
        jQuery("#orc_accountvar").autocomplete("close");
       
        orc_on_account_changed(jQuery("#accountvar").val());

        jQuery('#orc_gobutton').focus();

        orc_go_button_clicked();

        return false;
      }
    });

    // Setup the packageset changed event handler
    jQuery("#orc_pkgsetselid").on("change",function(event) {
      orc_on_packageset_changed(jQuery("#pkgsetselid").val());
    });

    // Setup the Go button event handler
    jQuery("#orc_gobutton").on("click", function() {
      orc_go_button_clicked();
    });

    // Create the model background div
    orc_config_create_model_div();

    orc_page_configuration_firsttime_setup_flag = true;
  }
}

// This function sets up the page configuration dialog
//   configtype - The initial configu type (site, account, or pricebook
//   account_or_pricebook - The name of the default account or pricebook
//   package - the name of the default package
//   variables, a comms seperate list of configuraiton variables to be displayed
//
function orc_setup_ordering_requirements(element, configtype, account_or_packageset, product_name, productid) {
  jQuery(element).data("ordering_requirements_variables",{"product_name": product_name, "productid": productid});

  orc_setup_page_global();

  jQuery('#ordering_requirements_config_header').html("Ordering Requirements Configuration&nbsp-&nbsp" + product_name);

  // Set the UI config type to the default
  jQuery("#orc_configtype").val(configtype);

  // Set the event handler for the config type change
  jQuery("#orc_configtype").on("change", function() {
    orc_set_config_type (jQuery("#orc_configtype").val()) ;
  });

  // Fire the event handler for the default config type, setting manually does not fire the event
  orc_set_config_type(((configtype != null) && (configtype != "")) ? configtype:"site");

  if (configtype == "pricebook") {
    jQuery("#orc_pkgsetselid").val(account_or_packageset);
  }

  // If the default config is account, set the account UI element
  if (configtype == "account") {
     jQuery("#orc_accountvar").val(account_or_packageset);
  }
}


