      //      only save notes visible to researchers if confirmed!
      accio_confirm({
        title: "Make Visible?",
        message: "Note for Researcher:<br>Are you sure you want to make this note visible to researchers?  If not, click no and uncheck the visible to researchers checkbox.",
        yeslabel: "Yes, Make Visible",
        nolabel: "No, Cancel",
        data: {masterid: masterid, subid: subid, val: val, v2r: v2r}
      },  "save_advancednote_confirmed");

      return(false);
    }
  }

  save_advancednote_confirmed(true,{masterid: masterid, subid: subid, val: val, v2r: v2r});
}

