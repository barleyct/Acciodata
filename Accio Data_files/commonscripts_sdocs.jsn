/*
  globalshow
    - shows a global variable in such a way that it can be
   clicked and edited if the page viewer is a sysop.
*/

function globalshow(globalname) {
  var onclickstr = '';
  if(sysopl2) {
    onclickstr = " class=blue10 onclick=\"globalset('"+globalname+"', this)\"";
  }
  var str="<span "+onclickstr+">"+eval(globalname)+"</span>";
  if(globalname.match(/EMAIL/)) {
    str="<span "+onclickstr+"><a href=mailto:"+eval(globalname)+">"+edittext('o5',true)+"</a></span>";
  }
  jQuery("#"+globalname).html(str);
//alert("globalshow done for "+globalname+"\n"+str);

}

/*
  globalset

  DESCRIPTION
     Saves changes to a system variable that was shown on the page
  with globalshow.
	
*/
function globalset(globalname, container) {
  document.changesitesettingsform.varname.value = globalname;
  document.changesitesettingsform.varvalue.value = eval(globalname);
  jQuery("#changesettings").css('display', 'block');

var x = findPosX(container);
  var y = findPosY(container);
  var id = document.getElementById('changesitesettings');
  id.style.display = 'block';


  if(navigator.appName == 'Microsoft Internet Explorer')     {
    id.style.top = y;
  }
  else {
    id.style.top = y;
  }

  x -= 700
  if(x<0) x=0;
  y += 15;

  moveDivTo('changesitesettings', x, y);

  return(false);
  
}

