    stdmodal_template  = jQuery("#accio-dialog-template").prop('outerHTML');
  }

  replacements = {};

  e = makestdcontainer_internal(containerid,'.modal-body',label,options,stdmodal_template.replace('accio-dialog-template','accio-dialog-' + (container_id++)),replacements);

  if (('classes' in options) && (e != null)) {
    jQuery(e).find('.accio-standard-dialog').addClass(options['classes']);
  }

  button = jQuery(e).find('.save-button')
  if (button.length > 0) {
    jQuery(e).find(".modal-footer").prepend(button[0]) ;
  }

  button = jQuery(e).find('.cancel-button')
  if (button.length > 0) {
    jQuery(e).find(".modal-footer .cancel-default").remove();

    jQuery(e).find(".modal-footer").append(button[0]) ;
  }

  restyle_elements(e);
  return(e);
}

/**
 * @return - new element created around existing element
 */
function makestdcontainer(containerid, label, options) {
  width = "";

  if ((arguments.length < 2) || (options == null)) {
    options = {}
  }

  if ("width" in options) {
    width = "width='" + options.width + "' ";
  }

  replacements = {"__WIDTH__": width};
  //console.log("Account Standard Container: " + jQuery("#accio-standard-container").length);

  if (stdcontainer_template == null) {
    stdcontainer_template = jQuery("#accio-standard-container").prop('outerHTML');
  }

  e = makestdcontainer_internal(containerid,".panel-body",label,options,stdcontainer_template.replace('accio-standard-container','accio_standard_container-' + (container_id++)),replacements);

  if (('width' in options) && (e != null)) {
    //console.log("Setting Width to: " + options['width']);

    jQuery(e).find('.accio-standard-container').width(options['width']);
  }

  if (('classes' in options) && (e != null)) {
    jQuery(e).find('.accio-standard-container').addClass(options['classes']);
  }
  restyle_elements(e);
  return(e);
}

function makestdcontainer_internal(containerid,containerclass, label, options, template, replacements) {
  var options = options || {};

  var e = null;

  if (containerid.indexOf('#') == 0) {
    jqElements = jQuery(containerid);
    if (jqElements.length > 0) {
      e = jqElements[0];
    }
  } else {
    e = document.getElementById(containerid);
  }

  if(e != null) {
    var content = e.innerHTML;
    //main use to set static width
    var style = options.style ? options.style : '';
    var insertelement = options.insertelement ? options.insertelement : e;

    // use innerHTML of a container_title as label if found
    jQuery( e ).find( 'div.container_title:first' ).each( function(index, element) {
      label = element.innerHTML;
    });
    //console.log("Title: " + label);
    replacements["@@label@@"] = label;
    replacements["@@content@@"] = (e != insertelement) ? content:"";

    str = template;
    for(var key in replacements) {
      if (replacements.hasOwnProperty(key)) {
        try {
          str = str.replace(new RegExp(key, 'g'), replacements[key])
        } catch (e) {
          start_debugger();
        }
      }
    }
    //console.log("Str: " + str);
    if (e != insertelement) {
      insertelement.innerHTML = str;
    } else {
      var children = jQuery(e).children();
      var contents = "";

      // Ok, an issue arose where the contents of the container had been manipulated via jquery (event handlers attached)
      // If we just grab the innerHTML and then append it to the newly minted container we wipe out the original dom content
      // and the new content will be detached from all the jQuery goofnedd that had previously been configured. To fix this
      // the code was modified to detach the inner DOM element then wipe insert the wrapper then re-insert the dom element
      // thereby preseving all the jquery stuff. A futher complication arose in situation where the contents had just previously
      // been instantiated and the DOM element had not yet been created, in this case the detach/reattached would error out, but
      // in that case no jquery would have been preformed, and we can use the old innerHTML method. Oy!
      if (typeof children.detach != "undefined") {
        contents = jQuery(e).children().detach();
        jQuery(e).html(str);
        jQuery(e).find(containerclass).append(contents);
      } else {
        contents = jQuery(e).html();
        jQuery(e).html(str);
        jQuery(e).find(containerclass).append(contents);
      }
    }
  } else {
    console.log("Could not find container: " + containerid);
  }

  return(e);
}

/**
 * Adds an open/close toggle function to the given accio standard container.
 *
 * @param {Element,string} container_id_or_elem containers id or element.
 *                                              **ALWAYS** better to use element to prevent a DOM scan.
 */
function add_toggle_to_stdcontainer(container_id_or_elem) {
    var $container;
    var containerid; 
    if( typeof containerid_or_elem == 'string' ) {
      $container = jQuery('#' + container_id_or_elem);
      containerid = $container.attr('id');
    } else {
      $container = jQuery(container_id_or_elem);
      containerid = container_id_or_elem;
    }

    var title_bar = $container.find(".container_title_bar").first();
    title_bar.html('<img id="'+ containerid + '_toggle" src="/pics/contract.gif" onclick=toggle_stdcontainer(\''+ containerid +'\')>&nbsp;' + title_bar.html());
}

function toggle_stdcontainer(containerid) {
    var container = jQuery("#" + containerid);
    var toggle = jQuery("#" + containerid + "_toggle");
    var toggle_gif = toggle.prop("src");
    if (toggle_gif.indexOf("contract") != -1) {
        toggle.prop("src","/pics/expand.gif");
        container.find(".container_center").first().parent().hide();
    } else {
        toggle.prop("src","/pics/contract.gif");
        container.find(".container_center").first().parent().show();
    }
}

