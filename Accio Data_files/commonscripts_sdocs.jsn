//
// This is a facility to display page sepcific global variables, and allow a user with high enough access
// to manipulate these variables closer to the locaiton where the variable affect the operaiton of the 
// system
//
// This code is included on just about all pages in the system. It works automatically by searching the page
// for elements with a class name of 'PageConfigurationButton' It then takes those elements and looks for a
// page_config_options attribute on each of the founds elements. This attribute specifies the name of a global
// javascript variable containing configuration information about which global variables are to be configured
// when the element of class 'PageConfigurationButton' is clicked. An on click handler is attached to the 
// element automatically when processed durring the jQuery page ready function.
//
// There can be multiple elements with the PageConfigurationButton class on any given page. The code gathers up
// the variables each element is configured to process, and creates a page level page configuration button
// in the menu bar allowing the user easy access to all variables on a page.
//
// To define a page variable configuration button, simply add a class to an HTML element of PageConfigurationButton:
//   <img style="display:none" id="var_config_gear" src=/pics/settings.png class=PageConfigurationButton page_config_options="var_config_options" />
//
// Then somewhere in the javascript define a global variable the corresponds the page_config_options specified in the element:
//    var <var_name>   = {configtype: <config_type>, configtypes: <config_types>, accountpackageset: <account_or_packageset>, package: <package>, variables: <variables>};
//
// var_name - global variable containing the information, this must match the value in the page_config_option attribute
//            of the PageConfigurationButton
//
// config_type - The default configuration type to start the UI with when this variable is being displayed. values are
//               site     `- Site Level Configuration
//               pricebook - Pricebook level configuration
//               account   - Account Level configuration
//               package   - Package level configuration
//
// config_types - an array of string config type which define which configuraiton types are valid for the specified variables
//                if this is not specified all configuration type are determined to be valid. this does not affect the page
//                level configuration button
//
// accountpackageset - The default name of the account of packageset whne displaying the UI for the specified variables
//
// variables - An array of global variables name that can be configured when the associated element is clicked, or 
//             when the page level config button is clicked.
//    
// Note: gears should start with the display:none option. The code will only show them if the user is logged as a sysopl2. 
// 
// Note: if you do not want the element show even after being process, surround the element in a div or span which is also
// display:none. Doing this allows you to define option that are only configured with the page level configuration button
// and do not have a gear dedicated to themselves.
//
// Note: Weather to display or not display a specified global variable at the specified site, packageset, account or package level
// is up to the code which generate the variable UI on the server. We do not specify anything to override this. For example
// if the variables array specifies 3 variables, 2 of which can be configured at the site and account level, and one that can
// only be configured at the account level, only the two variables configurable at the site level will show up if viewing
// the variables at the site level. If the UI is switched to the account level, all three of the variables would be displayed.
// 
// Note: while the class is called PageConfigurationButton, the element doe snot have to be a button, any element that
// can respond to a click event can be used. The system uses img element with a src of /pics/settings.png.
//
// Here is an example from MVR_quick.html
// 
//     <td>
//       <button type="button" name="PageConfiguration" id="PageConfigurationButton" class="PageConfigurationButton" value="Page Configuration"  text="Page Configuration" page_config_options="mvr_page_config_options" style="display:none">
//         <sPAn class=editabletxt id=rp>Page Configuration</sPAn>
//       </button>
//       <script>
//          var  mvr_page_config_options = {configtype: "site", accountpackageset: "", package: "", variables: ["SSN optional in Quick MVR","show_order_format_pdf"]};
//       </script>
//     </td>
//
// This is all that is needed to place a gear on a page.

var page_config_variables  = null;

////////////////////////////////////////////////////////////////////////////////////////////
// pvc_on_account_changed
//   This function  fires when the account is changed, it loades via ajax the packages related to the new company
//
// Arguments
//   account - the account the UI is changing to
//
// Returns
//   Nothing
//
// Results
//   The package drop down is populated with the specified accounts's list of packages
//
///////////////////////////////////////////////////////////////////////////////////////////
function pvc_on_account_changed(account) {

  jQuery.ajax({
    url: '/c/s2/get_account_packages_and_companyname',
    type: 'POST',
    data: { 'account': account }
  }).done(function(response) {
    if(response.match(/INVALID/)) {
      jQuery('#pvc_pkgselid').hide();
    } else {
      var rar = response.split('||');

      jQuery('#pvc_companyname').html(rar[0]);

      var html = "";

      for(var i=1; i < rar.length; i++) {
        html += '<option value="' + rar[i]+'">'+rar[i]+'</option>';
      }

      jQuery('#pvc_pkgselid').show();
      html += '<option value="Default" selected="selected">Default</option>';

      jQuery('#pvc_pkgselid').find('option').remove().end().append(html);
    }
  });
  pvc_config_selection_changed();

  pvc_enable_disable_go_button();
}

////////////////////////////////////////////////////////////////////////////////////////////
// pvc_on_packageset_changed
//   This function fires when the pricebook changes, it loads the packages from a global variable 
//
// Arguments
//   packageset - the newly selected packageset
//
// Returns
//   nothing
//
// Results
//   the package drop down is populated with the specified packageset's list of packages
//
////////////////////////////////////////////////////////////////////////////////////////////
function pvc_on_packageset_changed(packageset) {
  var packagelist = packageset_packages[packageset];
 
  var html = "";
  if (packagelist != null) {
    for(var idx = 0;idx < packagelist.length;idx++) {
      pname = packagelist[idx];
      html += '<option value="' + pname +'">'+pname+'</option>';
    }
  }
  jQuery('#pvc_pkgselid').show();

  html += '<option value="Pricebook-level" selected="selected">Pricebook-level</option>';

  jQuery('#pvc_pkgselid').find('option').remove().end().append(html);

  pvc_config_selection_changed();

  pvc_enable_disable_go_button();
}

////////////////////////////////////////////////////////////////////////////////////////////
// pvc_enable_disable_go_button
//   This function determines if enogh has been selected to enable the "Go" button and 
//   disables, or enables it.
//
// Arguments
//   none
//
// Returns
//   nothing
//
// Results
//   Enabled or Disables the Go Button based on the state of the UI
//
////////////////////////////////////////////////////////////////////////////////////////////
function pvc_enable_disable_go_button() {
  var enabled = false;
  switch (jQuery("#pvc_configtype").val()) {
    case "site":
      enabled = true;
      break;
    case "account":
      if (jQuery("#pvc_accountvar").val() != "") {
        enabled = true;
      }
      break;
    case "pricebook":
      if (jQuery("#pvc_pkgsetselid").val() != "") {
        enabled = true;
      }
      break;
  }  
  jQuery("#pvc_gobutton").prop("disabled",(enabled == false));
}

function pvc_config_selection_changed() {
  jQuery("#pageconfig_results_divid").html("<center>Please click &quot;Go&quot; to configure</center>");
  jQuery("#pageconfig_results_divid").css("display","block");

  jQuery(document).trigger("pvc-data-changing");
}

/////////////////////////////////////////////////////////////////////////////////////////////
// pvc_set_config_type
//   This function fires when the configuration type (Site, Account, Or Pricebook) changes. 
//   It takes care of showing or hiding different elements on the page as dictated by the config type chosen
//
// Arguments
//   the new configuraiton type (Site, Account, Or PriceBook)
//
// Returns
//   Nothing
//
// Results
//   this function adjusts the UI to reflect the new configuration type by hidding and showing only the fields
//   related to the new configuraiton type
//
//////////////////////////////////////////////////////////////////////////////////////////// 
function pvc_set_config_type(configtype) {
  //output2console("Config Type: " + configtype);
  switch (configtype) {
    case "account":
      jQuery("#pvc_accountvar").prop("disabled", false);
      jQuery("#pvc_accountvar").css("display",  "inline");
      jQuery("#pvc_pkgsetselid").prop("disabled",true);
      jQuery("#pvc_pkgsetselid").css("display", "none");
      jQuery("#pvc_accountpackageset_notapplicable").css("display","none");
      jQuery("#pvc_accountorpackagesettype").html("Account");

      jQuery("#pvc_pkgselid").prop("disabled",   false);
      jQuery("#pvc_pkgselid").css("display", "inline");
      jQuery("#pvc_package_notapplicable").css("display","none");

      pvc_on_account_changed(jQuery("#pvc_accountvar").val());
      break;
    case "pricebook":
      jQuery("#pvc_accountvar").prop("disabled", true);
      jQuery("#pvc_accountvar").css("display",  "none");
      jQuery("#pvc_pkgsetselid").prop("disabled",false);
      jQuery("#pvc_pkgsetselid").css("display", "inline");
      jQuery("#pvc_accountpackageset_notapplicable").css("display","none");
      jQuery("#pvc_accountorpackagesettype").html("Pricebook");

      jQuery("#pvc_pkgselid").prop("disabled",   false);
      jQuery("#pvc_pkgselid").css("display", "inline");
      jQuery("#pvc_package_notapplicable").css("display","none");

      pvc_on_packageset_changed(jQuery("#pkgsetselid").val());
      break;
    case "site":
    default:
      jQuery("#pvc_accountvar").prop("disabled", true);
      jQuery("#pvc_accountvar").css("display",  "none");
      jQuery("#pvc_pkgsetselid").prop("disabled",true);
      jQuery("#pvc_pkgsetselid").css("display", "none");
      jQuery("#pvc_accountpackageset_notapplicable").css("display","inline");

      jQuery("#pvc_pkgselid").prop("disabled",   true);
      jQuery("#pvc_pkgselid").css("display", "none");
      jQuery("#pvc_package_notapplicable").css("display","inline");

      break;
  } 

  pvc_config_selection_changed();

  pvc_enable_disable_go_button();
}

