function onDocumentLoaded(thefunction)
{
    if(window.addEventListener)
    {
        //All browsers, except IE before version 9.
        window.addEventListener("DOMContentLoaded", thefunction, false);
    } 
    else if(window.document.attachEvent)
    {
        //IE before version 9.
        window.document.attachEvent("onreadystatechange", thefunction);
    }
}

function accio_center(divid,left,top) {
  var stop          = jQuery(document).scrollTop();
  var sleft         = jQuery(document).scrollLeft();
  var window_width = jQuery(window).width();
  var dialog_width = jQuery("#" + divid).outerWidth();

  //output2console("Top: " + stop);
  jQuery("#" + divid).css('top',  stop   + top);
  jQuery("#" + divid).css('left', sleft  + (window_width/2) - (dialog_width/2) + left);
}

function accio_center_start(divid,leftoffset,topoffset,initial) {
  if ((arguments.length > 3) && (initial == true)) {
    accio_center(divid,leftoffset,topoffset);
  }

  jQuery(window).on("resize." + divid, null, {divid: divid, left: leftoffset, top: topoffset}, function(event) {
    accio_center(event.data.divid,event.data.left,event.data.top);
  });
}

function accio_center_stop(divid) {
  jQuery(window).off("resize." + divid);
}
//
// accio_status
//   Function to encapsulate the ajax status type stuff we do
//
// Parameters
//   elementid - ID of the element to contain the status message
//   level     - Level of the message, error, info, success, or warning
//   message   - The message to be displayed
//   options   - Hash of Extra options
//               clear - clears the element after specifed number of seconds
//               show  - Boolean indicates the the element should be shown or hidden
//
function accio_status(elementid, level, message, options) {
  jQuery("#" + elementid).html(message);

  classList = document.getElementById(elementid).className.split(/\s+/);
  for (var i = 0; i < classList.length; i++) {
    if (classList[i].indexOf("status_") === 0) {
      jQuery("#" + elementid).removeClass(classList[i]);
    }
  }

  if (level != "") {
    jQuery("#" + elementid).addClass("status_" + level);
  }


  if ("clear" in options)  {
    var id = "#" + elementid;
    var show = options["show"];

    setTimeout(function() {
      if (show == false) {
        jQuery(id).hide();
      }
      jQuery(id).html("");

    }, options["clear"] * 1000);
    jQuery(id).show();
  } else if ("show" in options)  {
    jQuery("#" + elementid).toggle(options["show"] == true);
  }
}
