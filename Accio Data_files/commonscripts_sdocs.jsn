/**
 * redirectAuthentication(authentication_url)
 *
 * Expire authentication and redirect via user click or after 5 seconds
 *
 * @param authentication_url
 */
function redirectAuthentication(authentication_url) {

    // disable global flag set in ews_func.js from showing warning when leaving
    appdoc_unload_warning_enabled = false;

    accio_alert(
        "Your session has expired.  To continue, you must re-authenticate.  Please <a href='" + authentication_url + "'>click here</a> to continue to authentication or wait to be redirected.", "Authentication has expired",
        500,
        {
            'modal': true,
            'callback': function () {
                window.location.href = authentication_url;
            }
        }
    );
    // Redirect no matter what after 30 seconds
    setTimeout(function() { window.location.href = authentication_url; }, 30000);
}

var t_update_authentication = null,
    t_check_authentication_expiration = null;

/**
 * runAuthenticationTaskBySecondsRemaining(request_masterid, request_key, authentication_url, callable)
 *
 * Responsible for determining the remaining seconds before expiration, and running callable with the result
 *
 * @param request_masterid
 * @param request_key
 * @param authentication_url
 * @param callable
 */
function runAuthenticationTaskBySecondsRemaining(request_masterid, request_key, authentication_url, callable) {
    jQuery.post(
        '/c/p/check_portal_authentication_expiration',
        {
            request_key: request_key,
            request_masterid: request_masterid
        },
        function (data) {
            // get seconds remaining ( will be less than 0 if expired )
            try { seconds_remaining = parseInt(data.seconds_remaining); } catch (e) { seconds_remaining = -1; }
            // redirect on expiration | Note: Assume expiration if there were any errors
            if (isNaN(seconds_remaining)) {
                // We will let the authentication page generate in the event we are having errors
                return;
            }
            if (seconds_remaining < 1) {
                redirectAuthentication(authentication_url);
            } else {
                // Schedule the next check according to the remaining time
                callable(request_masterid, request_key, authentication_url, (seconds_remaining * 1000));
            }
        },
        'json'
    );
}

/**
 * updateAuthentication(request_key, request_masterid)
 *
 * Update authentication time out, or start waiting for expiration
 *
 * @param request_key
 * @param request_masterid
 */
function updateAuthentication(request_masterid, request_key, authentication_url) {

    clearTimeout(t_update_authentication);
    clearTimeout(t_check_authentication_expiration);

    runAuthenticationTaskBySecondsRemaining(
        request_masterid,
        request_key,
        authentication_url,
        function (request_masterid, request_key, authentication_url, wait) {
            t_update_authentication = setTimeout(function () {
                    accio_confirm({
                        title: "Session expiring soon",
                        message: "Your session will expire in approximately 30 seconds.  Would you like to continue?",
                        yeslabel: "Yes, continue session"
                    }, function (confirmed, data) {
                        if (confirmed) {
                            jQuery.post(
                                '/c/p/update_portal_authentication',
                                {
                                    request_key: request_key,
                                    request_masterid: request_masterid
                                },
                                function (data) {
                                    if (data.success) {
                                        // authentication updated, call again for the next check
                                        updateAuthentication(request_masterid, request_key, authentication_url);
                                    } else {
                                        // too late, already expired
                                        redirectAuthentication(authentication_url);
                                    }
                                },
                                'json'
                            );
                        }
                        else {
                            // Start authentication checks to force expiration once reached
                            checkAuthenticationExpiration(request_masterid, request_key, authentication_url, 0);
                        }
                    });
                }, (wait - 30000)
            );
        }
    );
}

/**
 * checkAuthenticationExpiration(request_masterid, request_key, authentication_url, wait)
 *
 * If we reach checkAuthenticationExpiration, we are only waiting to expire
 *
 * @param request_masterid          Request id
 * @param request_key               Request guid
 * @param authentication_url        Authentication url
 * @param wait                      Time until next check
 */
function checkAuthenticationExpiration(request_masterid, request_key, authentication_url, wait) {
    runAuthenticationTaskBySecondsRemaining(
        request_masterid,
        request_key,
        authentication_url,
        function (request_masterid, request_key, authentication_url, wait) {
            t_check_authentication_expiration = setTimeout(function() {
                // Send to authentication URL if expired
                checkAuthenticationExpiration(request_masterid, request_key, authentication_url, wait);
            }, wait);
        }
    );
}

/**
 * Generate png image preview of document and display on page
 *
 * @param description   Document description
 * @param orderid       Order id
 */
function loadPreview(description, orderid, subid) {
    var preview_content = jQuery('#app_doc_preview_content'),
        preview_loading = jQuery('#app_doc_preview_loading'),
        // BUGS-7592 Added #app_doc_download_content_button element to the jquery selection so they both get the same html link generated for downloading attachment
        //           (the #app_doc_download_content_button element is only shown when screen size is too small (mobile version))
        download_content = jQuery('#app_doc_download_content,#app_doc_download_content_button');

    // Call to generate preview, this was done to allow time for lazy loading
    jQuery.post(
        '/c/p/preview_document',
        {
            'action': 'generate',
            'orderid': orderid,
            'subid': subid,
            'target': description
        },
        function (response) {
            var html = '';
            if (typeof response.count == "undefined" || response.count == 0) {
                // if nothing was generated, inform applicant
                html = '<div style="width:100%;line-height:200px;font-size:18pt;color: #CCCCCC; text-align:center;display:inline-block">Preview Unavailable</div>';
            } else {
                // generate markup for preview
                for (var i = 1; i <= response.count; i++) {
                    html += '<img style="max-width:100%" src="/c/p/preview_document?action=show&target=' + response.guid + '&page=' + i + '"><br>';
                }
            }
            // update preview state by hiding loader, and displaying preview content
            preview_loading.hide();
            preview_content.html(html);
            download_content.html('<a target="_blank" href="/c/p/preview_document?action=showpdf&target=' + response.guid + '">Download document</a>')
            preview_content.show();
        },
        'json'
    );
}

