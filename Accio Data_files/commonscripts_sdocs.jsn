}
/////////////////////////////////////////////////////////////////////////////////
// pkgsetselectedchanged
//   Handles the pricebook driop down changing  by setting up package dropdown
//
// Arguments
//   newpkgset - the new packageset/pricebook begin changed to
//   donotaddnew - flag indicating that the "Add New option should not be in the
//                 resulting package set
//   pricebook_level_default - Indicates if the "Prcebook-level option should be selected by default
//
// Returns
//    N/A
//
// Results
//     The elelemt identifier by pkgselinnerdivid get it content replaced with a package drop down
//
/////////////////////////////////////////////////////////////////////////////////
function pkgsetselectedchanged(newpkgset, donotaddnew, default_package) {
  var id = document.getElementById('pkgseldivid');

  if(newpkgset == '') {
    id.style.display = 'none';
    return;
  }
  id.style.display = 'block';
  var pkgid = document.getElementById('pkgselinnerdivid');
  var html = 'Package to edit :&nbsp;<select name=pkgsel id=pkgselid class="form-control" onChange="pkgselchanged(this.value);">';
  if(newpkgset != 'createnew') {
    var pkginfo = packageset_packages[newpkgset]; //when creating a new pricebook ... why would anyone wish to iterate over nil????
    if(pkginfo != null) {
      for(var i = 0; i < pkginfo.length; i++) {
        // Check to see if this is the default package and set it as selected if it is.
        selected = "";
        if ((arguments.length > 2 ) && (pkginfo[i] == default_package)) {
          selected = "selected";
        }
          
        html += '<option value="' + pkginfo[i]+'" ' + selected + '>'+pkginfo[i]+'</option>';
      }
    }
  }
  if(donotaddnew == null) {
    if(newpkgset != 'No_Pricebook_Selected') {
      if ((typeof(sysopl2) !== 'undefined')  && (sysopl2 != null)) {
        if(sysopl2 == true) {
          html += "<option value=createnew>Create New Package</option>";
        }
      }
    }
  }

  var dispstyle = 'style=display:none';
  if(newpkgset == 'createnew') {
    dispstyle = '';
    var npid=document.getElementById('newpackagedisplayid');
    if(npid != null) {
      npid.style.display = 'inline';
    }
  }

  html += '</select><input '+dispstyle+' name=newpackage id=newpackageid onchange="npidchg();">';

  /*
    XXX we need a new way to do this or to make this work in javascript...

---IF COURTPRO
  html = '<div style=display:none>' + html + '</div>'
---ENDIF COURTPRO
  */
  pkgid.innerHTML = html;

  id = document.getElementById('editpackagedivid');
  if(id != null) {
    id.style.display = 'none';
  }

  id=document.getElementById('newpackagesetdisplayid');

  if(id != null) {
    id.style.display = 'none';

    if(newpkgset == 'createnew') {
      id.style.display = 'inline';
    }
  }
}


function npidchg(idname) {
  if(idname == null) idname = 'newpackageid';
  var id = document.getElementById(idname);
  var val = id.value;

  if(val === '')
  {
    accio_alert("ERROR : You must enter a package name.","Empty Package Name",500);
    return false;
  }

  // Two checks: Invalid characters, and additional spacing
  if ((val.match(/^[a-zA-Z][a-zA-Z0-9 ]*$/) == null) ||
      (val.match(/\s{2,}/) != null)) {
    // Replace any non alpha-numberic characters
    val = val.replace(/[^a-zA-Z0-9 ]/g, ' ');
    // Replace first character with X if not alphabet character
    val = val.replace(/^[^a-zA-Z]/, 'X');
    // Replace any extra spacing from package name
    val = val.replace(new RegExp(/\s{2,}/g), ' ').trim();
    // Update value
    id.value = val;

    accio_alert("ERROR : The package name you have declared is invalid.<BR/>Package names must start with a letter and may contain only letters,<BR/>numbers, and single spacing.  The name you entered has been changed to meet these requirements.","Invalid Package Name",500);
    return false;
  }
  return true;
}

function pkgselchanged(newpkg) {
  var id = document.getElementById("editpackagedivid");
  if(id != null) id.style.display = "none";

  id=document.getElementById('newpackageid');
  if(id != null) {
    id.style.display = 'none';
    if(newpkg == 'createnew')
    id.style.display = 'inline';
  }
  id=document.getElementById('newpackagedisplayid');
  if(id != null) {
    id.style.display = 'none';
    if(newpkg == 'createnew')
    id.style.display = 'inline';
  }
}

function Select_Value_Set(SelectId, Value) {
  var id = document.getElementById(SelectId);
  if(id == null) return(-1);
  var index;
  for(index = 0; 
      index < id.length; 
      index++) {
    if(id[index].value == Value) {
      id.selectedIndex = index;
      return(index);
    }
  }
  // create a new option if it wasn't found in the list
  id.options[index]=new Option(Value, Value);
  id.selectedIndex = index;
  return(-1);
}


