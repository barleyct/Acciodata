// the input indicates which field needs to be sent with the form submittal
// If it is null, then we do not record the field, causing the form's entire contents
// to be sent this is required for pricing
function setneedtosave(formname,input) {
  window.onbeforeunload=warnunload;
  var id = document.getElementById(formname+'_need_to_saveid');
  if (input != null) {
    if (!(formname in things_to_save)) {
      things_to_save[formname] = []
    }
    if ((input.name == "CLEAR") && (input.type == "checkbox")) {
      if (typeof things_to_save[formname].clear == "undefined") {
        things_to_save[formname].clear = [];
      }
      things_to_save[formname].clear.push(input.value);
    }
    else if (things_to_save[formname].indexOf(input.name) == -1) {
      // console.log("Adding: " + input.name);
      things_to_save[formname].push(input.name);
    }
  }

  if(id != null) {
    id.value = '1';
  }

  makevisibleneedtosave();
}

// function save_question_set
// Params:  formname = name of the question form, question_type = component name
//
// This function is called when a question or answer is changed in a question set.  This marks the
// entire question set to be saved.
function save_question_set(formname, question_type) {
  var form_element=document.getElementById(formname);
  var rowcount = eval('questiontype_'+question_type+'_lastrowcount');

  for(var row = 1; row <= rowcount; row++) {
    var prompt =  'prompt_questiontype_' + question_type + '_' + row;
    if (jQuery(form_element).find('[name="' + prompt + '"]').val().strip().length > 0) {
      setneedtosave(formname, {name: prompt});
      var response =  'response_questiontype_' + question_type + '_' + row;
      setneedtosave(formname, {name: response});
      var required =  'required_questiontype_' + question_type + '_' + row;
      setneedtosave(formname, {name: required});
    }
  }

}

