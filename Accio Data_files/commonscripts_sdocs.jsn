  if (orc_page_configuration_firsttime_setup_flag == false) {
    //makestdcontainer("orc_pageconfigdivid","<table width='100%'><tr><td width='50%' id='ordering_requirements_config_header'>Ordering Requirements Configuration</td><td align='right' width='50%'><a href='javascript: orc_close_page_config(); void 0;'>[Close]</a></td></tr></table>");
