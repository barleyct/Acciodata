/** *****************************************************************
 * Extend jquery with this prepend class method.
 * This method will prepend the given classes in the string
 * to the classes already existing on the selected elements.
 * If the given classes are already on the element, the
 * classes will be duplicated - I.e. this does not ensure
 * the classes are not already there.
 * Example:
 * jQuery('button').not('.btn').prependClass('btn btn-default')
 * This would find all buttons that do not already have the
 * btn and bt-default classes, and add the class.
 * In this example, since the first selector is supported by
 * the browser, it runs really fast... and then the second
 * selector only has a small set of elements to examine.
 */
(function($) {
  jQuery.fn.extend({
    prependClass: function(newClasses) {
      return this.each(function(i) {
         var jquery_el = $(this)
         var currentClasses = jquery_el.prop("class");
         if(!currentClasses) { currentClasses = '' }
         jquery_el.removeClass();
         jquery_el.addClass(newClasses + " " + currentClasses);
         return true;
       });
     }
  });
})(jQuery);                                                                                                                                                                                                                                                +  36

/*Mon Jun  9 19:06:08 CDT 2008
 *In Conjunction with the Comet class found in /src/ruby_libraries/ajax_lib.rb, this function
 *is called periodically which makes the serverside request to SSPL (server side periodic listener)
 *and overrides the div with the new data received. This function is known as the Client Side
 *Periodic Listener.

 * If it receives the stop command, it acknowledges that it was received by the server can clean up
   files.
*/

function update_completion(guid_) {
  var div = document.getElementById('update_' + guid_);
  jQuery.post("/c/d/comet_read", "guid=" + guid_,
    function(data) {
      if (div != null && data != '')  div.innerHTML = data;
      regex = new RegExp("STOP " + guid_);
      if( data.match(regex) ){
        eval("clearInterval(intervalID_" + guid_ +")");
        jQuery.post("/c/d/comet_read", {'guid': guid_, 'stop': 'true'}, function(){})

        jQuery(document).trigger('comet-done',guid_);
      }
    }
  );
}
/***********************************************************************
 * This method will attempt to re-style all of the elements beneath it
 * to conform to some particular stylistic convention (such as
 * bootstrap 3).  Every element below the DOM element will be restyled
 * To restyle the entire page, do a restyle_elements(document.body)
 */
function restyle_elements (root) {
  var r_el = jQuery(root)
  r_el.find('button').not('.no-restyle').not('.btn').prependClass('btn btn-default')
  r_el.find('select').not('.no-restyle').not('.form-control').not('.form-control-sm').prependClass('form-control').wrap('<div class="form-group-sm"></div>')
  r_el.find('input[type=submit]').not('.no-restyle').not('.btn').not('navbar-toggle').prependClass('btn btn-default')
  r_el.find('input[type=button]').not('.no-restyle').not('.btn').not('navbar-toggle').prependClass('btn btn-default')
  /* the following line catches all inputs that have NO type set, texts and radios */
  r_el.find('input').not('.no-restyle').not('.form-control').not('.form-control-sm').not("[type='file']").not("[type='hidden']").not("[type='submit']").not("[type='button']").not("[type='checkbox']").not("[type='radio']").prependClass('form-control').wrap('<div class="form-group-sm"></div>')
  r_el.find('textarea').not('.no-restyle').not('.form-control').not('.form-control-sm').prependClass('form-control').wrap('<div class="form-group-sm"></div>')
  //r_el.find('input[size]').each( function(i) { var el = jQuery(this); el.css('width',''+ parseInt(el.attr('size'))+'em') } );
  r_el.find('div[class=divsubmit]').not('.no-restyle').not('.btn').prependClass('btn btn-default')
}
