  if (confirmed) {
    var masterid = data.masterid;
    var subid    = data.subid;

    jQuery.ajax({
      url:"/c/r/save_advancednote",
      data: { text :  data.val,
              request_masterid : data.masterid,
              visible_to_researchers : data.v2r,
              request_subid : data.subid
      },
      success: function(data) {
        jQuery("#advn_"+masterid+"_"+subid).find(".panel-body").html(data);
      }
    });
  }
