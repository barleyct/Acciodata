function jqsendform(formname, cginame) {
  var f=document.getElementById(formname);
  serialized = "";
  var dothereset = true;

  if (formname in things_to_save) {
    form_data = {};
    for(var idx = 0;idx < things_to_save[formname].length;idx++) {
      // If it s checkbox and checked, send back the value, otherwise send back blank
      if (jQuery(f).find('[name="' + things_to_save[formname][idx] + '"]').is(':checkbox')) {
        if (jQuery(f).find('[name="' + things_to_save[formname][idx] + '"]').is(':checked')) {
          form_data[things_to_save[formname][idx]] = jQuery(f).find('[name="' + things_to_save[formname][idx] + '"]').val();
        } else {
          form_data[things_to_save[formname][idx]] = "";
        }
      } else {
        form_data[things_to_save[formname][idx]] = jQuery(f).find('[name="' + things_to_save[formname][idx] + '"]').val();
      }
    }

    // If Present Load up the clear variables, We are banking of sysvar names not having commas
    if (typeof things_to_save[formname].clear != "undefined") {
      form_data["clear"] = JSON.stringify(things_to_save[formname].clear);
    }

    // Append any hidden values
    jQuery(f).find("input[type='hidden']").each (function(index,element) {
      form_data[element.getAttribute("name")] = element.value;
    });
    // format data for post
    serialized = jQuery.param(form_data, true);
  } else {
    dothereset = false;   // don't reset or we will then be in the things_to_save !
    serialized = jQuery(f).serialize();
  }

  jQuery.post(cginame+"?", serialized,
      function(response) {
        var is_done = response.match(/Done/),
            start_wizard = false;

        // check if we should start the wizard
        if ((!is_done) && (response.match(/hiddenpackage/))) {
          try { start_wizard = JSON.parse(response); } catch (e) { start_wizard = false; }
        }

        if(is_done || start_wizard) {
          postsavedresults(formname);
          // Reset the list of things needed to be saved only if in the list already
          if(dothereset) {
            if (typeof things_to_save[formname].clear != "undefined") {
              for(var idx = 0;idx < things_to_save[formname].clear.length;idx++) {
                var vname = things_to_save[formname].clear[idx];

                var checkbox = jQuery("input[type='checkbox'][value='" + vname + "']");

                checkbox.closest("tr").find("[name='" + vname + "']").val("");

                checkbox.closest("tr").find("[name='" + vname + "']").prop("disabled", false);

                checkbox.prop('checked',false);
              }
            }
            things_to_save[formname] = [];
          }

          if (saveas_refresh_flag == true) {
            window.location.href = "?pkgsetsel=" + saveas_packageset + "&newpackageset=&pkgsel=" + saveas_package + "&newpackage="
          }

          if (start_wizard) {
            start_recurring_monitor_wizard(start_wizard);
          }
        } else {
          postfailedsavedresults(formname, response);
        }
      }
  );
  return(false);  
}

