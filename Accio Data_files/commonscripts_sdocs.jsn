function pvc_setup_page() {
  load_packageset_list_items();

  // Setup the account autocomplete function
  jQuery("#pvc_accountvar").autocomplete({
    source:'/c/s2/complete_accountbyaccount',
    select: function(event,ui) {
      var selectedObj = ui.item;
      var val = selectedObj.value;
      var res = val.split('|');
      jQuery("#pvc_accountvar").val(res[0]);
      pvc_on_account_changed(res[0]);
      return false;
    }
  });

  // Setup the account changed event handler
  jQuery("#pvc_accountvar").on("change",function(event) {
    pvc_on_account_changed(jQuery("#pvc_accountvar").val());
  });

  jQuery("#pvc_accountvar").on("keydown",function(event){
    if (event.keyCode == 13) {
      event.preventDefault();
      
      jQuery("#pvc_accountvar").autocomplete("close");
     
      pvc_on_account_changed(jQuery("#pvc_accountvar").val());

      jQuery('#pvc_gobutton').focus();

      pvc_go_button_clicked();

      return false;
    }
  });

  // Setup the packageset changed event handler
  jQuery("#pvc_pkgsetselid").on("change",function(event) {
    pvc_on_packageset_changed(jQuery("#pvc_pkgsetselid").val());
  });

  // Setup the packageset changed event handler
  jQuery("#pvc_pkgselid").on("change",function(event) {
    pvc_config_selection_changed();
  });

  // Setup the Go button event handler
  jQuery("#pvc_gobutton").on("click", function() {
    pvc_go_button_clicked();
  });

  // Create the model background div
  pvc_config_create_model_div();
} 

