/**
 * Update required document signature
 *
 * @param mixed document_number
 * @param bool  success
 */
function updateRequiredDocumentSignature(document_number, subid, success) {
  var id = 'div#view_blankrelease_' + document_number + '_' + subid;
  // console.log("Id: " + id);
  var document_row = jQuery(id)
    // determine whether document requires signature
    var requires_signature = (document_row.data('view') == 'no');
    // default to successful
    var color = 'green';

    // update color if not successful
  if (!success) {
    color = (requires_signature) ? 'red' : 'yellow';
  }

  jQuery(id).attr('data-signed', 'yes');
  jQuery(id + ' > span').attr('class', ('appdocsign' + color + 'class'));
  try {
      var sum_status = jQuery("#sign_doc_menu_id_"+document_number + "_" + subid + "_sum_status");
      if (success) {
          sum_status.attr('class', 'label label-success');
          sum_status.find('span[data-success]').show();
          sum_status.find('span[data-danger]').hide();
      } else {
          sum_status.attr('class', 'label label-danger');
          sum_status.find('span[data-danger]').show();
          sum_status.find('span[data-success]').hide();
      }
  } catch (e) {}
}

/**
 * Update required document view
 *
 * @param mixed document_number
 * @param bool  success
 */
function updateRequiredDocumentView(document_description, document_number, subid, success) {
    var cookieName = 'viewed_' + jQuery('[name="guikey"]').val();
    var viewed = getCookie(cookieName);

    // default viewed if not yet set
    if (viewed == null) {
        viewed = '';
    }
    // add to viewed
    viewed = viewed.split(';');
    viewed.push(document_description + ":" + subid);
    // marked as signed (no signature needed)
    updateRequiredDocumentSignature(document_number, subid, success);
    // set cookie
    setCookie(cookieName, viewed.join(';'));
}
