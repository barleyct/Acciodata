function load_pageconfig_html() {
  if (jQuery("#page_variable_config_dialog").length == 0) {
    pvc_pageconfig_html_loading = true; 

    jQuery.ajax("/sysops/page_variable_config.html").done(function(results) {
      jQuery('body').append(results);

      //load_packageset_list_items();

      pvc_pageconfig_html_loaded = true;

      if (jQuery("#pvc_configtype").val() == "pricebook") {
        pvc_on_packageset_changed(jQuery("#pkgsetselid").val());
      }
    });
  } else {
    pvc_pageconfig_html_loaded = true;
  } 
}

function enable_page_configuration_buttons() {
  buttons = jQuery(".PageConfigurationButton[processed!=1]");
  buttons.on("click", function(event) {
    if (pvc_pageconfig_html_loaded == false) {
      if (pvc_pageconfig_html_loading == false) {
        load_pageconfig_html();
      }
      return;
    }

    var setup_done = jQuery(this).data("setup_done");

    if ((setup_done == null) || (setup_done == false)) {
      var options_var = jQuery(this).attr("page_config_options");
      if (options_var != null) {
        var options = window[options_var];

        if (options != null) {
          configtypes = ["site","account","pricebook"]
          if (Object.keys(options).includes("configtypes")) {
            configtypes = options.configtypes
          }
          pvc_setup_page_config(this, options.configtype, configtypes, options.accountpackageset, options["package"], options.variables, options.message);

          jQuery(this).data("setup_done",true);
        } else {
          return;
        }
      } else {
        return;
      }
    }

    if (this != pvc_last_config) {
      pvc_config_selection_changed();
    }

    pvc_show_page_config(this);
  });
  buttons.attr("processed","1");

  jQuery.each(buttons, function(index,element) {
    var events = jQuery._data(element,"events");
  });

  buttons.css("display","inline");
}

// This function fire on the document ready and looks fo rall elements with a class of
// PageConfiguraitonButton, for each of those elements it attaches an event handler to 
// the "click" event.
//
// When the click event is fired it firs tlooks to see if the element has been initialized
// If not initialized it looks for a "page_config_options" attribute to find the name of
// a global variable containing the setup information for the config dialog.
//
// The setup information consists of and object with the following members:
//    configtype: The initial configuration type, "site", "account" or "pricebook"
//    accountpricebook: The default account name or pricebook name depending of if the 
//                       configtype is  "account" or "pricebook", ignored it the configtype is "site"
//    package: The name of the default package, ignored if the configtype is "site", may be blank
//    variables: An array of variable names to be displayed for page configuration
//
jQuery(document).ready(function() {
  var setup_done = false;
  if (sysopl2 == true) {
    create_page_configuration();
    enable_page_configuration_buttons();
  }
});
