var copy_template_index = 0;

function acciopkgcopy(divid, fname, pkgset, pkgname, packagesets, callback) {
  var did = document.getElementById(divid);
  //output2console("DIV ID: " + divid);
  var id = '';
  if (jQuery("#" + divid).find(".copy-table").length == 0) {
    var current_index = copy_template_index;

    id = 'copy-dialog-'+current_index;
    var copydialog = jQuery("#package-copy-template").clone().prop('id', id);
    jQuery("#" + divid).append(copydialog);
    jQuery("#" + id).find(".copy-type").html((pkgname == "")? "Pricebook" : "Package");
    jQuery("#" + id).find(":input[name='system_variables']").prop('id','copy-system-variables-' + current_index);
    jQuery("#" + id).find(":input[name='questions']").prop('id','copy-questions-' + current_index);
    jQuery("#" + id).find(":input[name='copy-confirm']").prop('id','copy-confirm-' + current_index);
    jQuery("#" + id).find(":input[name='copy-cancel']").prop('id','copy-cancel-' + current_index);
    jQuery("#" + id).find("form[name='copy-package']").prop("id",'copy-package-form' + current_index);

    jQuery("#" + id).find("input[name='from_package']").val(pkgname);
    jQuery("#" + id).find("input[name='from_packageset']").val(pkgset);

    if (packagesets != null) {
      var options = "";
      for(var idx = 0;idx < packagesets.length;idx++) {
        options += "<option value='" + packagesets[idx] + "'>" + packagesets[idx].replace(/_/," ") + "</option>";
      }
      jQuery("#" + id).find("select[name='to_packageset']").html(options);
      jQuery("#" + id).find("select[name='to_packageset']").val(pkgset);
    } else {
      jQuery("#" + id).find(".pricebook-row").hide();
    }
    var title = "Copy " + ((pkgname == "")? "Pricebook: " + pkgset : "Package: " + pkgname);
    makestdcontainer(id, title);

    jQuery("#copy-cancel-" + current_index).on("click" ,function() {
      jQuery("#" + id).hide();
      hide_modal_overlay('package_overlay');
    });

    jQuery("#copy-confirm-" + current_index).on("click" ,function() {
      show_modal_overlay('package_overlay');
      show_waiting_dialog('Please Wait');

      jQuery.ajax({url: "/c/s2/copypackage", data: jQuery("#copy-package-form" + current_index).serialize(), success: function(results) {
        if (results.match(/^error:/i)) {
          accio_alert(results, "Error Copying Package", 600, {} );
        } else if (results.match(/^success/i)) {
          if (callback != null) {
            callback(jQuery("#copy-package-form" + current_index).find("select[name='to_packageset']").val(),jQuery("#copy-package-form" + current_index).find("input[name='to_package']").val());
          } else {
            accio_alert("Package Successfully Copied", "Package Copied", 600, {} );
          }
        } else {
          accio_alert(results.replace(/ERROR(\s+)?:(\s+)?/, ''), "Error", 600, {} );
        }
      }}).fail(function() {
        accio_alert("An unexpected internal error occurred copying the package, please retry", "Internal Error", 600, {} );
      }).always(function() {
        jQuery("#" + id).hide();
        hide_modal_overlay('package_overlay');
        hide_waiting_dialog();
      });
    });

    jQuery("#" + id).show();

    jQuery("#" + id).css("top", (jQuery(window).scrollTop() + 75) + "px");

    copy_template_index += 1;
  } else {
    id = jQuery("#" + divid).find(".copy-dialog").prop('id');
  }
  //did.style.display="block";
  jQuery("#" + id).show();
  show_modal_overlay('package_overlay');
}


