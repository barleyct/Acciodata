// This function sets up the page configuration dialog
//   configtype - The initial configu type (site, account, or pricebook
//   account_or_pricebook - The name of the default account or pricebook
//   package - the name of the default package
//   variables, a comms seperate list of configuraiton variables to be displayed
//
function pvc_setup_page_config(element, configtype, configtypes, account_or_packageset, package_name, variables, message) {
  jQuery(element).data("page_config_variables",variables);
  page_config_variables = variables;

  pvc_setup_page_global();

  options = ""
  for(var idx = 0;idx < configtypes.length;idx++) {
    options += "<option value='" + configtypes[idx] + "'>" + configtypes[idx] + "</options>";
  }
  jQuery("#pvc_configtype").html(options);
    
  // Set the UI config type to the default
  jQuery("#pvc_configtype").val(configtype);

  // Set the event handler for the config type change
  jQuery("#pvc_configtype").on("change", function() {
    pvc_set_config_type (jQuery("#pvc_configtype").val()) ;
  });

  // Fire the event handler for the default config type, setting manually does not fire the event
  pvc_set_config_type(((configtype != null) && (configtype != "")) ? configtype:"site");

  if (configtype == "pricebook") {
    jQuery("#pvc_pkgsetselid").val(account_or_packageset);
  }

  // If the default config is account, set the account UI element
  if (configtype == "account") {
     jQuery("#pvc_accountvar").val(account_or_packageset);
  }

  if ((message == null) || (message.length == 0)) {
    jQuery("#pvc_message").hide();
  } else {
    jQuery("#pvc_message").html(message);
    jQuery("#pvc_message").show();
  }

  pvc_enable_disable_go_button();
}

