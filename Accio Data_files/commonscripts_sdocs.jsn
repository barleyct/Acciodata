  }
  id=document.getElementById('questiontype_'+questiontype);
  if(id != null) {
    id.style.display = 'inline';  // must be inline not block
  }
}


function getpackagefromform(formname) {
  var f=document.getElementById(formname+'_packageid');
  name = jQuery(f).serialize();
  return(name);
}


function getpackagesetfromform(formname) {
  var f=document.getElementById(formname+'_packagesetid');
  name = jQuery(f).serialize();
  return(name);
}


function delete_question_row(questiontype, rowcnt, formname) {

  var pid = document.getElementById('id_prompt_questiontype_'+questiontype+'_'+rowcnt);
  var rid = document.getElementById('id_response_questiontype_'+questiontype+'_'+rowcnt);

  // We use param here instead of serialize because the server side expects and is simpler if the questionprompt and questionresponse parameters
  // come in without the questiontype and the rowcnt appended. The param method adds the expected encoding for the string
  // If there is no formname passed here, then don't delete question from database as it was not in database to begin with.  Just remove from UI.
  if(formname != "") {
    var params = getpackagesetfromform(formname);
    params += '&' + getpackagefromform(formname);
    params += '&' + jQuery.param({questionprompt: jQuery(pid).val()}, true);
    params += '&' + jQuery.param({questionresponse: jQuery(rid).val()}, true);
    params += '&questiontype='  + questiontype;
    jQuery.post("/c/s2/delete_question?", params,
      function(response) {}
    );
  }

  if(pid != null) {
    pid.disabled = true;
  }
  if(rid != null) {
    rid.disabled = true;
  }
  id = document.getElementById('questionrow_'+questiontype+'_'+rowcnt);
  if(id != null) {
    jQuery(pid).val('');
    jQuery(rid).val('');
    id.style.display = 'none';
  }
}


function delete_questiontype(questiontype, formname) {

  // delete from DB
  var params = getpackagesetfromform(formname);
  params += '&' + getpackagefromform(formname);
  params += '&questiontype=' + questiontype;
  jQuery.post("/c/s2/delete_question?", params,
      function(response) {}
  );

  var rowcount = eval('questiontype_'+questiontype+'_lastrowcount');
  for(var i = 1; i <= rowcount; i++) {
    delete_question_row(questiontype, i, "");
  }

  var id=document.getElementById('delete_questiontype_'+questiontype);
  if(id != null) {
    id.style.display = 'none';
  }
  id=document.getElementById('add_questiontype_'+questiontype);
  if(id != null) {
    id.style.display = 'block';  // must be block not inline
  }
  id=document.getElementById('questiontype_'+questiontype);
  if(id != null) {
    id.style.display = 'none';
  }
}


function add_more_questions(questiontype, formname) {
  var rowcnt = eval("questiontype_"+questiontype+"_lastrowcount");
  var id = document.getElementById('table_body_'+questiontype+'_id');
  if(id != null) {
    var extrarows = rowcnt + 5;
    while(rowcnt < extrarows) {
      rowcnt += 1;
      var tr = document.createElement("TR"); 
      id.appendChild(tr);
      tr.setAttribute("id", "questionrow_"+questiontype+"_"+rowcnt);

      var td = document.createElement("TD"); 
      tr.appendChild(td);
      td.innerHTML = "<span onclick='if(confirm(\"Are you sure you want to delete this question? Can not be undone.\")) delete_question_row(\""+questiontype+"\", \""+rowcnt+"\", \""+formname+"\")' class=\"blue12\" onmouseover=\"this.style.cursor='pointer'\">Delete</span>";

      var td1 = document.createElement("TD"); 
      tr.appendChild(td1);
      td1.innerHTML = "<textarea class='form-control' id=\"id_prompt_questiontype_"+questiontype+"_"+rowcnt+"\" name=\"prompt_questiontype_"+questiontype+"_"+rowcnt+"\" rows=\"2\" cols=\"40\" onChange='setneedtosave(\""+formname+"\",this);'></textarea>";

      var td2 = document.createElement("TD"); 
      tr.appendChild(td2);
