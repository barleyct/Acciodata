 */

/**
 * method to implement the 'required_if' rule for LIVR validation
 *
 * @param {Object} query - the entire LIVR rule generated by the LIVR validation library to examine the values of the fields
 *
 * @return {null|string} null if no errors - An identification string if a rule failed
 */
function livr_required_if(query) {
    var queryKey;
    var queryValue;
    var util = LIVR.util;

    if (arguments.length > 1) {
        queryKey = Object.keys(query)[0];
        queryValue = query[queryKey];

        if (typeof queryValue != 'string' || !util.isPrimitiveValue(queryValue)) {
            throw new Error(
                'LIVR: the target value of the "require_if" rule is missed or incomparable'
            );
        }
    }

    /* function livr_required_if_internal is essentially a lambda method
       returned to the caller to validate a field
    */
    function livr_required_if_internal (value, params) {
        if (!util.isNoValue(value) || !queryKey) return;

        var valueToCheck = LIVR_JSONPointer(params, queryKey);

        if (valueToCheck == queryValue && util.isNoValue(value)) return 'REQUIRED';

        return;
    };

    return(livr_required_if_internal);
}

/**
 * method to implement the 'required_if_other_field_regexp' rule for LIVR validation
 *
 * @param {Object} query - the entire LIVR rule generated by the LIVR validation library to examine the values of the fields
 *
 * @return {null|string} null if no errors - An identification string if a rule failed
 */
function livr_required_if_other_field_regexp(query) {
    var queryKey;
    var queryValue;
    var util = LIVR.util;

    if (arguments.length > 1) {
        queryKey = Object.keys(query)[0];
        queryValue = query[queryKey];

        if (!queryValue || !util.isPrimitiveValue(queryValue)) {
            throw new Error(
                'LIVR: the target value of the "require_if_other_field_regexp" rule is missed or incomparable'
            );
        }
    }

    /* function livr_required_if_internal is essentially a lambda method
       returned to the caller to validate a field
    */
    function livr_required_if_other_field_regexp_internal (value, params) {
        if (!util.isNoValue(value) || !queryKey) return;

        var valueToCheck = LIVR_JSONPointer(params, queryKey);

        var re = new RegExp(queryValue);
        if (re.test(valueToCheck) && util.isNoValue(value)) return 'REQUIRED';

        return;
    };

    return(livr_required_if_other_field_regexp_internal);
}

/**
 * Part of the implementation of the required_if rule
 */
function LIVR_JSONPointer(object, pointer) {
        var parts = pointer.split('/');
        var value = object;

        var i = 0;
        for (;i<parts.length;i++) {
            if (!value) break;
            value = value[parts[i]];
        }

        return value;
    }

/**
 * method to invoke LIVR field validation
 *
 * This method assumes there is a global verify on the window
 * called livr_rules that contains rules for the fields to
 * be validated.
 *
 * @param {array|object} fields a Javascript object (hash) containing the list of fields to validate
 * @param {object} rules        A javascript hash (probably from a JSON object) containing the rules for the fields.  The rules
 *                              must adhere to the LIVR spec.
 *
 * @return {object} 
 *                  'errors_as_string' member has an empty string if there were no errors, and a string with errors separated by commas if there were any.
 *                  'fields_with_errors' is another object with fields that have errors as their keys, and the corresponding LIVR error code
 *                  i.e.
 *                  {
 *                    'errors_as_string' : 'first_name is required',
 *                    'fields_with_errors' : { 'first_name': 'REQUIRED' } 
 *                  }
 *
 */
function invoke_by_livr(fields,rules) {
  var return_value = { 'errors_as_string' : '', 'fields_with_errors' : {} };
  if(!window || !window.LIVR) { return(return_value); } // only supported on the browser right now

  var error_strings = {
    'REQUIRED': "_FIELD_ is required",
    'CANNOT_BE_EMPTY': "_FIELD_ cannot be empty",
    'FORMAT_ERROR': "_FIELD_ value of _VALUE_ has the wrong format",
    'NOT_ALLOWED_VALUE': "_FIELD_ value of _VALUE_ is not allowed",
    'TOO_SHORT': "_FIELD_ value of _VALUE_ is too short",
    'TOO_LONG': "_FIELD_ value of _VALUE_ is too long",
    'WRONG_FORMAT': "_FIELD_ value of _VALUE_ has the wrong format",
    'NOT_INTEGER': "_FIELD_ value of _VALUE_ is not an integer",
    'NOT_POSITIVE_INTEGER': "_FIELD_ value of _VALUE_ is not a positive integer",
    'NOT_DECIMAL': "_FIELD_ value of _VALUE_ is not a decimal number",
    'NOT_POSITIVE_DECIMAL': "_FIELD_ value of _VALUE_ is not a positive decimal number",
    'TOO_HIGH': "_FIELD_ value of _VALUE_ is too high",
    'TOO_LOW': "_FIELD_ value of _VALUE_ is too low",
    'WRONG_EMAIL': "_FIELD_ value of _VALUE_ is an invalid email address",
    'WRONG_DATE': "_FIELD_ value of _VALUE_ is an invalid date",
    'WRONG_URL': "_FIELD_ value of _VALUE_ is an invalid URL",
    'FIELDS_NOT_EQUAL': " _FIELD_ is not equal to _FIELD2_",
  };  

  LIVR.Validator.defaultAutoTrim(true);  // automatically trim fields
  var validator = new LIVR.Validator(rules);
  var data = validator.validate(fields);
  if(!data) {
    var errors = validator.getErrors();
    for(var field in errors) {
      if(errors.hasOwnProperty(field)) {
        return_value['fields_with_errors'][field] = errors[field];
        if(errors[field] == 'FIELDS_NOT_EQUAL') {
          return_value['errors_as_string'] += error_strings['FIELDS_NOT_EQUAL'].replace('_FIELD_',field).replace('_FIELD2_',errors[field]['equal_to_field']) + " ";
        } else {
          return_value['errors_as_string'] += error_strings[errors[field]].replace('_FIELD_',field).replace('_VALUE_',fields[field]) + " ";
        }
      }
    }
  }
  return(return_value);
}

/**
 * method to take a byid_verification array (which consists of field names on even
 * indices and labels on odd indicies, and set those fields to bad item status
 * if the fields do not validate using verification_by_livr.
 *
 * @note This method assumes something else already set the fields to good
 *       item status.
 *
 * @note Assumes there is a global object on the windows object livr_validation_rules.
 *
 * @param {array} fields An array of form field ids where the ids are on the even
 *                array indices and the field labels are on the odd array indices.
 *
 * @side-effect This method will mark fields bad-item (changes to the DOM).
 *
 * @return {string} either an empty string (no errors) or a string with comma delimeted errors
 */
function verify_by_livr(fields) {
  var rules = window.livr_validation_rules;
  if(!rules) { return ''; }
  var rules_for_livr = {};

  var i;
  var field_value_hash = {};
  var elements = {};
  for(i=0;i<fields.length;i+=2) {
    var field = fields[i];
    var element = document.getElementById(field);
    if(element) {
      elements[field] = element;
      if(!rules[field]) { continue; }
      rules_for_livr[field] = rules[field];
      field_value_hash[field] = element.value;
    }
  }
  // validate the fields
  var livr_result = invoke_by_livr(field_value_hash,rules_for_livr);

  // update good bad status on elements to bad if necessary
  for(var erroneous_field in livr_result['fields_with_errors']) {
    var element = elements[erroneous_field];
    if(livr_result['fields_with_errors'].hasOwnProperty(erroneous_field) && element) {
      set_element_goodbad_status(element, bad_item_css)
    }
  }
       
  return(livr_result['errors_as_string']);
}
