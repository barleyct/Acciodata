// The opposite of setneedtosave - remove an element from the things_to_save
function unsetneedtosave(formname,input) {
  var arr = things_to_save[formname];
  if(arr) {
    var idx = arr.indexOf(input.name);
    if(idx !== -1) {
      arr.splice(idx, 1);
    }
    if(arr.length === 0) {
      delete things_to_save[formname];
      // below is the opposite of makevisibleneedtosave();
      var id = document.getElementById(formname+'_need_to_saveid');
      if(id != null) {
        if(id.value == '1') {
          var fields = ['save_status_', '', 'save_status_', '_end'];
          for(var j=0; j < fields.length; j+=2) {
            var fname = fields[j]+formname+fields[j+1];
