function process_orc_click(element) {
  var setup_done = jQuery(element).data("setup_done");

  if ((setup_done == null) || (setup_done == false)) {
    // console.log("Setup Not Done");
    var options_var = jQuery(element).attr("ordering_requirements_options");
    if (options_var != null) {
      var options = window[options_var];

      if (options != null) {
        orc_setup_ordering_requirements(element, options.configtype, options.accountpackageset, options.productname, options.productid);

        jQuery(element).data("setup_done",true);
      } else {
        return;
      }
    } else {
      return;
    }
  }

  if (element != orc_last_config) {
    orc_config_selection_changed();
  }

  orc_show_ordering_requirements(element);
}

