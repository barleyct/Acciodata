function load_packageset_list_items() {
  var html = "";

  // Load the pricebook
  for(var idx = 0;idx < package_list.length;idx++) {
    pname = package_list[idx];

    html += '<option value="' + pname + '">'+pname+'</option>';
  }
  jQuery('#pvc_pkgsetselid').find('option').remove().end().append(html);
