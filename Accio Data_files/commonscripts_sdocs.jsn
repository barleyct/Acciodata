function pvc_go_button_clicked() {
  var post_data = {};

  switch (jQuery("#pvc_configtype").val()) {
    case "pricebook":
      post_data = {pkgsetsel: jQuery("#pvc_pkgsetselid").val(), pkgsel:jQuery("#pvc_pkgselid").val()};
      break;
    case "account":
      post_data = {accountvar: jQuery("#pvc_accountvar").val(), pkgsel:jQuery("#pvc_pkgselid").val()};
